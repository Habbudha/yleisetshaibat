package ht2;

public class Ls1Velallinen  {
	private int id;
	private String nimi;
	private int numero;
	
	public Ls1Velallinen(int id, String nimi, int numero) {
		this.id = id;
		this.nimi = nimi;
		this.numero = numero;
	}
	
	public int getNumero() {
		return this.numero;
	}
	public String getNimi() {
		return this.nimi;
	}
	public int getId() {
		return this.id;
	}
	public String toString() {
		return getId() + " " + getNimi() + " " + getNumero();
	}
	public void setNimi(String nimi) {
		this.nimi = nimi;
	}
	public void setNumero(int numero) {
		this.numero = numero;
	}
	
	
}
