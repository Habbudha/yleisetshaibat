<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" >
   <top>
      <Label text="LISÄÄ VELKA" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="216.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Velan summa" />
            <TextField>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </TextField>
            <Label text="Velanottaja" />
            <HBox prefHeight="30.0" prefWidth="214.0">
               <children>
                  <TextField prefHeight="25.0" prefWidth="185.0" />
                  <MenuButton mnemonicParsing="false" prefHeight="25.0" prefWidth="31.0" text="*">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Lando" />
                      <MenuItem mnemonicParsing="false" text="Hab" />
                    </items>
                  </MenuButton>
               </children>
            </HBox>
            <Label text="Puhelinnumero" />
            <TextField />
            <Label text="Sovittu maksupäivä" />
            <TextField />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <Button mnemonicParsing="false" text="Tallenna" BorderPane.alignment="CENTER">
         <graphic>
            <Button mnemonicParsing="false" text="Peruuta" />
         </graphic>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
   <left>
      <Pane prefHeight="200.0" prefWidth="79.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Pane prefHeight="200.0" prefWidth="79.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
